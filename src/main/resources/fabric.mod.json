{
  "schemaVersion": 1,
  "id": "orion",
  "version": "${version}",
  "name": "Orion",
  "description": "A combat addon for Meteor Client.",
  "authors": [
    "GhostTypes",
    "Cloudburst",
    "Crosby"
  ],
  "contact": {
    "repo": "https://github.com/GhostTypes/orion"
  },
  "icon": "assets/orion/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.ghosttypes.orion.Orion"
    ]
  },
  "mixins": [
    "orion.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "251, 96, 139",
    "github:sha": "@gh_hash@"
  },
  "depends": {
    "java": ">=16",
    "minecraft": ">=@mc_version@",
    "meteor-client": "*"
  }
}
